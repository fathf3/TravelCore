
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("~/Views/MemberLayoutPartial/HeaderPartial.cshtml")

<body class="g-sidenav-show  bg-gray-100">
  @await Html.PartialAsync("~/Views/MemberLayoutPartial/NavbarPartial.cshtml")
  <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
    <!-- Navbar -->
    @await Html.PartialAsync("~/Views/MemberLayoutPartial/NavbarUstPartial.cshtml")
    <!-- End Navbar -->
    <div class="container-fluid py-4">
     @RenderBody()
      @await Html.PartialAsync("~/Views/MemberLayoutPartial/FooterPartial.cshtml")
    </div>
  </main>
  @await Html.PartialAsync("~/Views/MemberLayoutPartial/RightBarPartial.cshtml")
  <!--   Core JS Files   -->
  @await Html.PartialAsync("~/Views/MemberLayoutPartial/ScriptPartial.cshtml")
</body>

</html>


